{
  "name": "node-html-parser",
  "version": "2.2.6",
  "description": "A very fast HTML parser, generating a simplified DOM, with basic element query support.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "mocha",
    "build": "gulp",
    "pretest": "gulp",
    "prepare": "gulp"
  },
  "keywords": [
    "fast html parser nodejs typescript"
  ],
  "author": "Xiaoyi Shi <ashi009@gmail.com>",
  "contributors": [
    "taoqf<tao_qiufeng@126.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "he": "^1.2.0"
  },
  "devDependencies": {
    "@types/entities": "^1.1.1",
    "@types/he": "^1.1.1",
    "@types/node": "^11.15.20",
    "blanket": "^1.2.3",
    "del": "^4.1.1",
    "gulp": "^4.0.2",
    "gulp-sequence": "^1.0.0",
    "gulp-shell": "0.7.0",
    "mocha": "^6.2.3",
    "should": "^13.2.3",
    "spec": "^1.0.1",
    "travis-cov": "0.2.5",
    "typescript": "^3.5.1"
  },
  "config": {
    "blanket": {
      "pattern": "./dist/index.js",
      "data-cover-never": [
