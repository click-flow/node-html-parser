      "threshold": 70
    }
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taoqf/node-fast-html-parser.git"
  },
  "bugs": {
    "url": "https://github.com/taoqf/node-fast-html-parser/issues"
